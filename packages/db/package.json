    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0"
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "prisma": "^4.8.0",
    "eslint": "^8.30.0"
  }
}
